<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>RequestAPI</servlet-name>
        <servlet-class>com.example.servl01.RequestAPI</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RequestAPI</servlet-name>
        <url-pattern>/re</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Paramater</servlet-name>
        <servlet-class>com.example.servl01.Paramater</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Paramater</servlet-name>
        <url-pattern>/pa</url-pattern>
    </servlet-mapping>

  <servlet>
      <servlet-name>Servlet01</servlet-name>
      <servlet-class>com.example.servl01.Servlet01</servlet-class>
  </servlet>
    <servlet-mapping>
        <servlet-name>Servlet01</servlet-name>
        <url-pattern>/s01</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Servlet02</servlet-name>
        <servlet-class>com.example.servl01.Servlet02</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Respon01</servlet-name>
        <servlet-class>com.example.servl01.Respon01</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Respon02</servlet-name>
        <servlet-class>com.example.servl01.Respon02</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet02</servlet-name>
        <url-pattern>/s02</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Respon01</servlet-name>
        <url-pattern>/res1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Respon02</servlet-name>
        <url-pattern>/res2</url-pattern>
    </servlet-mapping>

</web-app>