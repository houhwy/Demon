<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="user" class="IOC.gyb.user"></bean>
    <bean id="book" class="IOC.gyb.Book">
        <property name="bname" value="龙族"></property>
        <property name="bauthor" value="江南老贼"></property>
    </bean>
    <bean id="order" class="IOC.gyb.Ordeers">
        <constructor-arg name="oname" value="wenwen"></constructor-arg>
        <constructor-arg name="adress" value="luoyang"></constructor-arg>
    </bean>
</beans>